{% set cacheClear = random(9999999) %}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Langland | Admin{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset("style/admin/build.css?v="~cacheClear) }}" />
        <link rel="stylesheet" href="{{ asset('style/admin/theme/bootstrap-minty.min.css?v='~cacheClear) }}">
    {% endblock %}

</head>
<body>
<div id="vue_admin">
    {% block body %}{% endblock %}
</div>

{% block javascripts %}
    <script
            src="https://code.jquery.com/jquery-3.3.1.min.js"
            integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
            crossorigin="anonymous"></script>

    <script src={{ asset("admin/dist/build.js?"~cacheClear) }}></script>
{% endblock %}

</body>
</html>